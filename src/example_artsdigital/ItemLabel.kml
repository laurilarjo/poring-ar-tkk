<Document>

	<Style id="undecorated_style">
		<BalloonStyle>
			<displayMode>undecorated</displayMode> <!-- no styling applied to balloon -->
		</BalloonStyle>
	</Style>
	
	<Placemark>
		<name>Unstyled Placemark Example</name>
		<snippet>working example for vta demo of an item label</snippet>
		<description><![CDATA[ 		<style type="text/css">
		
		.labelcontainer	{ position:absolute; left: 0px;}
		
		.indicator	{position: absolute; left: 150px; top:49px;height: 60px; 	
					width: 30px; padding: 10px 5px 0px 10px;z-index:3;background-image:url(http://www.artsdigital.com/ar_browser/images/pointer_1.png); background-position:left top;background-repeat:no-repeat}
					
		.label		{	position: absolute; left: 50px; top:5px;height: 35px; 
					width: 260px; padding: 10px 5px 0px 10px;	z-index:1;		font-family:	 Helvetica,sans-serif; font-size: 10px; letter-spacing:0.1em; text-align: left; background-color: rgba(255, 255, 255, 1.0);					color: rgb(0,0,0); -webkit-border-radius: 50px; -webkit-box-shadow: 5px 5px 10px rgba(0,0,0,0.6);				} 
					
	.labeltext	{	position: absolute; left: 67px; top:16px;height: 25px; 
					width: 200px;	z-index:5;		font-family:	 Helvetica,sans-serif; font-size: 9px; letter-spacing:0.1em; text-align: left; 					color: rgb(0,0,0);overflow:hidden;				} 
					
		.itemthumb	{width:45px; height:34px;position: relative; top: 
						11px;right:-260px;z-index:3}
						
		.presence	{width:15px; height:15px;position: relative; top: 
						-3px;right:-240px;z-index:4}
					
					
		</style>
		
			<div id="labelcontainer">
					
					<div class="indicator">
					
					
					</div>			
					
					<span class="label">
					
					
					</span>	
					
					<span class="labeltext">
					
					GT Conference Center
					
					</span>		
					
				
					
							<img src="http://www.artsdigital.com/ar_browser/images/itemthumb.jpg" class="itemthumb"/>
							
							<img src="http://www.artsdigital.com/ar_browser/images/presence_green.png" class="presence"/>
							
					
					
				</div>
        ]]>
		
		
		</description>
		<balloonVisibility>1</balloonVisibility>
		<Balloon>
			<locationMode>relative</locationMode>
			<location>
				<latitude units="meters">-6.0</latitude> <!-- southwest of the user -->
				<longitude units="meters">-6.0</longitude> <!-- southwest of the user -->
				<altitude>0.0</altitude>
			</location>
			<orientationMode>billboard</orientationMode>
		</Balloon>
		<styleUrl>#undecorated_style</styleUrl> <!-- reference to style element -->
	</Placemark>
	
