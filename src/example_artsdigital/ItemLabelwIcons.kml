<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
<Document>

	<Style id="undecorated_style">
		<BalloonStyle>
			<displayMode>undecorated</displayMode> <!-- no styling applied to balloon -->
		</BalloonStyle>
	</Style>
	
	<Placemark>
		<name>Unstyled Placemark Example</name>
		<snippet>working example for vta demo of an item label</snippet>
		<description><![CDATA[ 
		
		<style type="text/css">
		
		
.labelcontainer { position:absolute; left: 0px;}

.indicator		{ 	position: absolute; left: 0px; width: 35px; height:				
					35px; padding: 5px; margin: 5px 20px 7px 0px;
					background-color: rgba(255, 255, 255, 0.8); -webkit-border-radius: 10px;
					-webkit-box-shadow: 5px 5px 10px rgba(0,0,0,0.6)			}
					
.indicator_selected { 	position: absolute; left: 0px; width: 35px; height:				
					35px; padding: 5px; margin: 5px 20px 7px 0px;
					background-color: rgba(255, 255, 255, 0.8); -webkit-border-radius: 10px;
					-webkit-box-shadow: 5px 5px 10px rgba(0,0,0,0.6)			}
					
.label			{	position: absolute; left: 50px; top:5px;height: 35px;
					width: 260px; padding: 10px 5px 0px 10px;			font-family:	 Helvetica,sans-serif;
					font-size: 10px; letter-spacing:0.1em; text-align: left; background-color: rgba(255, 255, 255, 0.8);					color: rgb(0,0,0);
					-webkit-border-radius: 10px;
					-webkit-box-shadow: 5px 5px 10px rgba(0,0,0,0.6)				}		
						
		
.icon_flickr			{	position: absolute; left: 260px;top:30px; width: 
							24px; height: 24px; padding: 5px;
							margin: 5px 20px 7px 0px; }
						
						
.icon_youtube		{	position: absolute; left: 30px; top: -5px; width: 24px; 
						height: 24px;
						padding: 5px; margin: 5px 20px 7px 0px; }
						
						
		</style>
						
						
					<div class="labelcontainer">
						
						<div class="indicator">
						
						<img src="http://www.artsdigital.com/ar_browser/images/dotm.png"/>
						
						</div>
						
						
						<span class="label">
						
						GT Conference Center
						
						</span>		
						
						</div>
					
        ]]>
		
		
		</description>
		<balloonVisibility>1</balloonVisibility>
		<Balloon>
			<locationMode>relative</locationMode>
			<location>
				<latitude units="meters">-6.0</latitude> <!-- southwest of the user -->
				<longitude units="meters">-6.0</longitude> <!-- southwest of the user -->
				<altitude>0.0</altitude>
			</location>
			<orientationMode>billboard</orientationMode>
		</Balloon>
		<styleUrl>#undecorated_style</styleUrl> <!-- reference to style element -->
	</Placemark>
	
		<Placemark>
		<name>Unstyled Placemark Example</name>
		<snippet>working example for vta demo of an item label</snippet>
		<description><![CDATA[ 
		
		
						<div class="labelcontainer">
						
